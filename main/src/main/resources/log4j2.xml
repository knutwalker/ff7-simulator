<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
 <Appenders>
  <Console name="Console" target="SYSTEM_OUT">
   <PatternLayout pattern="[%d{HH:mm:ss.SSS} %4.4level %16.16logger{16}] - %msg%n"/>
  </Console>
 </Appenders>
 <Loggers>
  <Root level="info">
   <AppenderRef ref="Console"/>
  </Root>
 </Loggers>
</Configuration>
